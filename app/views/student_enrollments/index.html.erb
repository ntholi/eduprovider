<h1>My Enrollments</h1>

<%= link_to 'New Enrollment', new_student_enrollment_path %>
<br/><br/>

<% @student_enrollments.each do |student_enrollment| %>
<article class="course-box">
  <%= link_to student_enrollment, student_enrollment %>
  <div class="course-box-menu">
    <%= link_to 'Unregister', student_enrollment, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
</article>
<% end %>
